
<div id="${html_id}" class="slickgrid-wrapper">

</div>

<script type="text/javascript">
    // wiring will be postponed to tail
    (window.sdiTailLast = window.sdiTailLast || []).push(
        function wireGrid() {
            var html_id = '${html_id}';
            window.console && console.log &&
                // Show in the debugger that we have correct jQuery.
                console.log('in-panel wiring\n\npanel = grid, html_id = ', html_id, ', jQuery = ', $);
        }
    );
</script>

<hr>

<table class="table table-striped">
    <thead>
    <tr>
        <th tal:repeat="c columns">${c.title}</th>
    </tr>
    </thead>
    <tbody>
    <tr tal:repeat="i items">
        <td tal:repeat="c columns">
            ${i[c.name]}
        </td>
    </tr>
    </tbody>
</table>
